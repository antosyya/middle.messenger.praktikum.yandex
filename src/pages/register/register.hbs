<main>
    <h1>Вход</h1>
    <from class="form" id="register-form">
        <label for="name">Имя</label>
        {{> Input id="name" type="text" name="name" placeholder="имя"}}    
     
        <label for="lastname">Фамилия</label>
        {{> Input id="lastname" type="text" name="lastname" placeholder="фамилия"}}    
      
        <label for="login-register">Логин</label>  
        {{> Input id="login-register" type="text" name="login-register" placeholder="логин"}}

        <label for="phone">Телефон</label>
        {{> Input id="phone" type="tel" name="phone" placeholder="телефон"}}    

        <label for="password">Пароль</label>
        {{> Input id="password" type="password" name="password" placeholder="пароль"}}    
     
        <label for="password-two">Повторите пароль</label>
        {{> Input id="password-two" type="password" name="password" placeholder="пароль"}}    
        <button class="button" type="submit">Зарегистироваться</button>
    </from>
</main>